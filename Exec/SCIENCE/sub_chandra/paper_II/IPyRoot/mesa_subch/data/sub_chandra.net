         r34_pp3             ! he4(he3 g)be7(p g)b8(e+ nu)be8( a)he4  
         r_h1_he3_wk_he4               ! he3(p e+nu)he4     

         rbe7ec_li7_aux 
         rbe7pg_b8_aux 



         )


