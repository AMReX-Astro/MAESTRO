&PROBIN

 use_eos_coulomb = T
 
/


&params 

 nx = 10240
 dens_base = 4.5d9
 temp_base = 6.0d8

 dens_conv_zone = -1.0
 M_conv_zone = 1.0d0

 low_density_cutoff = 5.d-2
 temp_fluff = 1.8d7

 fluff_type = "continuous"

 test_hse_convergence = T
 TOL = 3.d-10
 MAX_ITER = 10000000
 dtol_fac = 1.0d-6

 xmin = 0.0
